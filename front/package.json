{
	"name": "front",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "CLOUDFLARE_ENV=local react-router dev",
		"typecheck": "pnpm run cf-typegen && react-router typegen && tsc -b",
		"deploy:dev": "CLOUDFLARE_ENV=dev pnpm run build && wrangler deploy",
		"deploy:prd": "CLOUDFLARE_ENV=prd pnpm run build && wrangler deploy",
		"build": "react-router build",
		"cf-typegen": "wrangler types -e local",
		"postinstall": "npm run cf-typegen",
		"preview": "pnpm run build && vite preview"
	},
	"dependencies": {
		"@tanstack/react-query": "^5.81.5",
		"hono": "^4.7.2",
		"isbot": "^5.1.27",
		"react": "^19.1.0",
		"react-dom": "^19.1.0",
		"react-router": "^7.5.3"
	},
	"devDependencies": {
		"@cloudflare/vite-plugin": "^1.0.12",
		"@react-router/dev": "^7.5.3",
		"@tailwindcss/vite": "^4.1.4",
		"@tanstack/react-query-devtools": "^5.81.5",
		"@types/node": "^20",
		"@types/react": "^19.1.2",
		"@types/react-dom": "^19.1.2",
		"tailwindcss": "^4.1.4",
		"typescript": "^5.8.3",
		"vite": "^6.3.3",
		"vite-tsconfig-paths": "^5.1.4",
		"wrangler": "^4.23.0"
	}
}