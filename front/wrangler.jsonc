{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "todo-front",
	"compatibility_date": "2025-04-04",
	"main": "./workers/app.ts",
	"observability": {
		"enabled": true
	},
	"env": {
		"dev": {
			"name": "todo-front-dev",
			"vars": {
				"VALUE_FROM_CLOUDFLARE": "Hello from Cloudflare Dev"
			}
		},
		"prd": {
			"name": "todo-front-prd",
			"vars": {
				"VALUE_FROM_CLOUDFLARE": "Hello from Cloudflare Production"
			}
		}
	}
}